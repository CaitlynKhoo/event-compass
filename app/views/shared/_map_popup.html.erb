<div class="card" style="width: 18rem;">
  <%= image_tag event.img_url, class: "card-img-top" %>
  <div class="card-body">
    <p class="card-text"><strong><%= event.name %></strong></p>
    <p class="card-text"><%= event.location %></p>
    <p class="card-text"><strong><%= event.price %></strong></p>
  </div>
</div>
